    "        \n",
    "    def __len__(self):\n",
    "        return len(self.features)\n",
    "\n",
    "    def __getitem__(self, idx):\n",
    "        lon = self.features.loc[idx].longitude\n",
    "        lat = self.features.loc[idx].latitude\n",
    "        img_name = str(lon) + \"_\" + str(lat) + \".jpg\"\n",
